{
  "name": "kpn-style-portal",
  "description": "Portal with documentation and examples for KPN Style, which is used within the Business Market.",
  "version": "1.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/kpn/kpn-style.git"
  },
  "homepage": "https://style.kpn.com",
  "private": false,
  "author": "KPN",
  "license": "MIT",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "static": "node scripts/static.js",
    "lint": "tslint -p config/tsconfig.tslint.json -t stylish -c config/tslint.json",
    "stylelint": "stylelint \"packages/**/*.scss\" \"portal/**/*scss\" --quiet --syntax scss"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@svgr/webpack": "^5.1.0",
    "@types/fs-extra": "^8.0.1",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "a11y": "^0.5.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.3",
    "bfj": "^7.0.2",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "chalk": "^3.0.0",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "^5.0.2",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "4.0.0-beta.11",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "jest-pnp-resolver": "^1.2.1",
    "jest-resolve": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pnp-webpack-plugin": "^1.6.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.1",
    "react-app-polyfill": "^1.0.6",
    "react-dev-utils": "^10.1.0",
    "resolve": "^1.15.0",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "stylelint": "^12.0.1",
    "terser-webpack-plugin": "^2.3.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-dev-server": "^3.10.2",
    "webpack-manifest-plugin": "^2.2.0",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "dependencies": {
    "html-to-react": "^1.4.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-ga": "^2.7.0",
    "react-helmet": "^5.2.1",
    "react-markdown": "^4.3.1",
    "react-router-dom": "^5.1.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  }
}
